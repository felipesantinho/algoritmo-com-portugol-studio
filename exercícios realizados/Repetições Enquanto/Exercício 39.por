programa
{
		
	funcao inicio()
	{

	inteiro contador
	 
	contador = 10
   
   	enquanto ((contador <= 10) e (contador >= 3)) {
     	escreva(contador)
     	contador = contador - 1
		escreva ("\t")
   	}
   	
   	escreva("\n\nAcabou!\n")
	   								
	}
}
